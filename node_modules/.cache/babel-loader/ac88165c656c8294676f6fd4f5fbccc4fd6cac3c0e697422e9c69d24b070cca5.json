\n            this.isAuthenticated = false\n            this.username = ''\n        },\n        login() {\n            if (this.username !== ''){\n                this.isAuthenticated = true\n                localStorage.setItem('isAuthenticated', true)\n                localStorage.setItem('username', this.username)\n            }\n        }\n    },\n    created() {\n        this.isAuthenticated = false\n        this.username = ''\n        localStorage.removeItem('isAuthenticated')\n        localStorage.removeItem('username')\n    }\n}\n</script>\n\n<style>\nnav {\n    display: flex;\n    justify-content: space-between;\n    align-items: center;\n    background-color: darkseagreen;\n    padding: 20px;\n}\n</style>"],"mappings":"AAyBA,eAAe;EACXA,IAAI,EAAE,UAAU;EAChBC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,eAAe,EAAE,KAAK;MACtBC,QAAQ,EAAE;IACd;EACJ,CAAC;EACDC,OAAO,EAAE;IACLC,MAAMA,CAAA,EAAG;MACL,IAAI,CAACH,eAAc,GAAI,KAAI;MAC3B,IAAI,CAACC,QAAO,GAAI,EAAC;IACrB,CAAC;IACDG,KAAKA,CAAA,EAAG;MACJ,IAAI,IAAI,CAACH,QAAO,KAAM,EAAE,EAAC;QACrB,IAAI,CAACD,eAAc,GAAI,IAAG;QAC1BK,YAAY,CAACC,OAAO,CAAC,iBAAiB,EAAE,IAAI;QAC5CD,YAAY,CAACC,OAAO,CAAC,UAAU,EAAE,IAAI,CAACL,QAAQ;MAClD;IACJ;EACJ,CAAC;EACDM,OAAOA,CAAA,EAAG;IACN,IAAI,CAACP,eAAc,GAAI,KAAI;IAC3B,IAAI,CAACC,QAAO,GAAI,EAAC;IACjBI,YAAY,CAACG,UAAU,CAAC,iBAAiB;IACzCH,YAAY,CAACG,UAAU,CAAC,UAAU;EACtC;AACJ"},"metadata":{},"sourceType":"module","externalDependencies":[]}