')\n            localStorage.removeItem('username')\n        },\n        login() {\n            if (this.username !== ''){\n                this.isAuthenticated = true\n                localStorage.setItem('isAuthenticated', true)\n                localStorage.setItem('username', this.username)\n                this.$router.push({\n                    name: 'chat',\n                    query: {username: this.username}\n                })\n            }\n        }\n    },\n    created() {\n        if (localStorage.getItem('isAuthenticated')){\n            this.isAuthenticated = true\n            this.username = localStorage.getItem('username')\n        }\n    }\n}\n</script>\n\n<style>\nh1 {\n    margin: 0;\n    padding: 15px;\n}\nmain {\n    height: 400px;\n    width: 300px;\n    margin: 30vh auto;\n    display: flex;\n    flex-direction: column;\n    align-items: center;\n    justify-content: center;\n}\nbutton {\n    width: 100px;\n    height: 50px;\n    font-size: 15px;\n    background-color: rgb(180, 212, 104);\n}\n</style>"],"mappings":";AAeA,eAAe;EACXA,IAAI,EAAE,UAAU;EAChBC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,eAAe,EAAE,KAAK;MACtBC,QAAQ,EAAE;IACd;EACJ,CAAC;EACDC,OAAO,EAAE;IACLC,MAAMA,CAAA,EAAG;MACL,IAAI,CAACH,eAAc,GAAI,KAAI;MAC3B,IAAI,CAACC,QAAO,GAAI,EAAC;MACjBG,YAAY,CAACC,UAAU,CAAC,iBAAiB;MACzCD,YAAY,CAACC,UAAU,CAAC,UAAU;IACtC,CAAC;IACDC,KAAKA,CAAA,EAAG;MACJ,IAAI,IAAI,CAACL,QAAO,KAAM,EAAE,EAAC;QACrB,IAAI,CAACD,eAAc,GAAI,IAAG;QAC1BI,YAAY,CAACG,OAAO,CAAC,iBAAiB,EAAE,IAAI;QAC5CH,YAAY,CAACG,OAAO,CAAC,UAAU,EAAE,IAAI,CAACN,QAAQ;QAC9C,IAAI,CAACO,OAAO,CAACC,IAAI,CAAC;UACdX,IAAI,EAAE,MAAM;UACZY,KAAK,EAAE;YAACT,QAAQ,EAAE,IAAI,CAACA;UAAQ;QACnC,CAAC;MACL;IACJ;EACJ,CAAC;EACDU,OAAOA,CAAA,EAAG;IACN,IAAIP,YAAY,CAACQ,OAAO,CAAC,iBAAiB,CAAC,EAAC;MACxC,IAAI,CAACZ,eAAc,GAAI,IAAG;MAC1B,IAAI,CAACC,QAAO,GAAIG,YAAY,CAACQ,OAAO,CAAC,UAAU;IACnD;EACJ;AACJ"},"metadata":{},"sourceType":"module","externalDependencies":[]}